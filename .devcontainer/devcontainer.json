{
    "name": "tensorflow",
    "image": "moreih29/tensorflow-gpu:v1.6",
    "extensions": [
        "ms-python.python",
	    "mutantdino.resourcemonitor",
        "alphabotsec.vscode-eclipse-keybindings",
    ],
    "settings": { 
		"python.languageServer": "Pylance",
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": false
        },
	},
    "runArgs": ["--name", "tf_gpu", "--gpus", "all"],
}
